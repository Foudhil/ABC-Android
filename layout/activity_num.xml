<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/num"
    tools:context=".NUM">

    <ImageView
        android:id="@+id/homeTest"
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/home_button" />

    <TextView
        android:id="@+id/homeNum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="8dp"
        android:layout_marginBottom="279dp"
        android:fontFamily="casual"
        android:text="HOME"
        android:textAlignment="center"
        android:textColor="@android:color/black"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/homeTest"
        app:layout_constraintVertical_bias="0.0" />

    <ImageView
        android:id="@+id/n0"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="38dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/num0" />

    <ImageView
        android:id="@+id/n1"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="38dp"
        app:layout_constraintStart_toEndOf="@+id/n0"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/num1" />

    <ImageView
        android:id="@+id/n2"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="40dp"
        android:layout_marginTop="38dp"
        app:layout_constraintStart_toEndOf="@+id/n1"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/num2" />

    <ImageView
        android:id="@+id/n3"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="128dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/n0"
        app:srcCompat="@drawable/num3" />

    <ImageView
        android:id="@+id/n4"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toEndOf="@+id/n3"
        app:layout_constraintTop_toBottomOf="@+id/n1"
        app:srcCompat="@drawable/num4" />

    <ImageView
        android:id="@+id/n5"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toEndOf="@+id/n4"
        app:layout_constraintTop_toBottomOf="@+id/n2"
        app:srcCompat="@drawable/num5" />

    <ImageView
        android:id="@+id/n6"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="130dp"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/n3"
        app:srcCompat="@drawable/num6" />

    <ImageView
        android:id="@+id/n7"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toEndOf="@+id/n6"
        app:layout_constraintTop_toBottomOf="@+id/n4"
        app:srcCompat="@drawable/num7" />

    <ImageView
        android:id="@+id/n8"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toEndOf="@+id/n7"
        app:layout_constraintTop_toBottomOf="@+id/n5"
        app:srcCompat="@drawable/num8" />

    <ImageView
        android:id="@+id/n9"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:layout_marginStart="220dp"
        android:layout_marginTop="25dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/n7"
        app:srcCompat="@drawable/num9" />

</android.support.constraint.ConstraintLayout>